<div id="<%= dom_id post %>">
	<p>
		<strong>Title:</strong>
		<%= post.title %>
	</p>
	<p>
		<strong>Body:</strong>
		<%= post.body %>
	</p>
	<p>
		<strong>likes:</strong>
		<% post.likes.order(created_at: :desc).limit(5).each do |like| %>
			<p>
				<%= like.user.email %>
			</p>
		<% end %>
	</p>
</div>
